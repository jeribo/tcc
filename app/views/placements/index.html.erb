<h1>Listing placements</h1>

<table>
  <tr>
    <th>Type address</th>
    <th>Address</th>
    <th>City</th>
    <th>District</th>
    <th>Complement</th>
    <th>Zip</th>
    <th>State</th>
    <th>Person</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @placements.each do |placement| %>
  <tr>
    <td><%= placement.type_address %></td>
    <td><%= placement.address %></td>
    <td><%= placement.city %></td>
    <td><%= placement.district %></td>
    <td><%= placement.complement %></td>
    <td><%= placement.zip %></td>
    <td><%= placement.state %></td>
    <td><%= placement.person %></td>
    <td><%= link_to 'Show', placement %></td>
    <td><%= link_to 'Edit', edit_placement_path(placement) %></td>
    <td><%= link_to 'Destroy', placement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Placement', new_placement_path %>
